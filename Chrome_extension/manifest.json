{
    "name": "RAIVEN",
    "version": "1.0",
    "description": "A source credibilty checker!",
    "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "http://localhost:8080/*"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "16": "images/RAIVEN16px.png"
        }
    },
    "icons": {
        "16": "images/RAIVEN16px.png"
    },
    "manifest_version": 2
}